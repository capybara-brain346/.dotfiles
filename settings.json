{
  "workbench.iconTheme": "bearded-icons",
  "C_Cpp.default.compilerPath": "C:\\MinGW\\bin\\gcc.exe",
  "git.openRepositoryInParentFolders": "never",
  "security.allowedUNCHosts": ["wsl.localhost"],
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font Mono",
  "editor.fontLigatures": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "git.enableSmartCommit": true,
  "liveServer.settings.donotVerifyTags": true,
  "code-runner.runInTerminal": true,
  "git.ignoreMissingGitWarning": true,
  "workbench.settings.applyToAllProfiles": ["editor.fontFamily"],
  "editor.fontFamily": "JetBrainsMono Nerd Font Mono",
  "editor.codeLensFontFamily": "Cascadia Code",
  "debug.console.fontFamily": "Cascadia Code",
  "terminal.integrated.tabs.defaultColor": "terminal.ansiWhite",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "[css]": {
    "editor.suggest.insertMode": "replace"
  },
  "workbench.colorCustomizations": {},
  "editor.minimap.enabled": false,
  "editor.fontSize": 13,
  "accessibility.dimUnfocused.opacity": 1,
  "git.terminalGitEditor": true,
  "editor.fontWeight": "normal",
  "terminal.integrated.fontWeight": "bold",
  "extensions.ignoreRecommendations": true,
  "git.autofetch": true,
  "window.titleBarStyle": "custom",
  "editor.accessibilitySupport": "off",
  "editor.fontVariations": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.confirmSync": false,
  "files.autoSave": "afterDelay",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.systemColorTheme": "dark",
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "files.associations": {
    "*.html": "html",
    "*.py": "python"
  },
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "explorer.compactFolders": false,
  "workbench.sideBar.location": "right",
  "workbench.editorLargeFileConfirmation": 9000,
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "apc.font.family": "JetBrainsMono Nerd Font Mono",
  "python.createEnvironment.trigger": "off",
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "workbench.productIconTheme": "material-product-icons",
  "editor.wordWrap": "on",
  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  },
  "go.toolsManagement.autoUpdate": true,
  "redhat.telemetry.enabled": false,
  "workbench.startupEditor": "none",
  "terminal.integrated.allowedLinkSchemes": [
    "file",
    "http",
    "https",
    "mailto",
    "vscode",
    "vscode-insiders",
    "docker-desktop"
  ],
  "diffEditor.hideUnchangedRegions.enabled": true,
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "editor.lineNumbers": "relative",
  "java.configuration.runtimes": [],
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "workbench.activityBar.location": "hidden",
  "window.menuBarVisibility": "compact",
  "settingsSync.ignoredSettings": [],
  "workbench.colorTheme": "GitHub Dark",
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
  "workbench.statusBar.visible": false
}
